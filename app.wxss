/*End 评奖*/

.header_bd {
  line-height: 1.8;
  text-align: center;
  overflow: hidden;
  padding-top: 10px;
  width: 100%;
  font-size: 14px;
  color: #fff;
}

.header_bd .user {
  font-weight: 600;
  box-sizing: border-box;
  padding: 5px;
  width: 100%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  color: #fff;
}

