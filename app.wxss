.view-content {
  padding: 0 15px;
  color: #231815;
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
  flex-grow: 1;
}

.view-content .content_logo {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  margin: 20px 0;
  color: #fff;
  font-size: 16px;
  font-weight: 800;
  letter-spacing: 3px;
}

.view-content .content_logo image {
  width: 157px;
  height: 30px;
}

.content_lists {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
  justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  margin: 20px 0;
  flex-direction: column;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  width: 700rpx;
  height: 600rpx;
}
.content_num{
display:flex;
width:100%;
justify-content:space-around;
box-sizing:border-box;
padding:0 40rpx;
align-items: center;

}
.content_lists .lists_hd {
  width: 40rpx;
  color: #3e3a39;
}

.content_lists .lists_bd {
  font-weight: 500;
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
  flex-grow: 1;
}

.content_lists .lists_bd image {
  vertical-align: middle;
  margin: 0 15px;
  width: 40px;
  min-width: 40px;
  height: 40px;
  border-radius: 50%;
  border: 1rpx solid #ccc;
  background-color: #c7caca;
}

.content_lists .lists_ft {
  color: #73daff;
  font-size: 15px;
}

.content_lists:last-child {
  padding-bottom: 0;
}

.share_bar {
  margin: 30px 15px;
}

.adds_bar {
  width: 690rpx;
