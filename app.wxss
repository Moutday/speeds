/*评奖*/

.ranking {
  position: absolute;
  left: -20rpx;
  right: 0;
  margin: 0 auto;
  width: 160rpx;
  height: 40rpx;
  z-index: 1;
  top: 180rpx;
}

.ranking_0 {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  bottom: -10px;
}

.ranking_1 {
  background: url(data:image/png;base64,) no-repeat;
